<UserProfileWidget>:
    orientation: 'vertical'
    Label:
        text: f"Nome: {root.user_data['name']}"
    Label:
        text: f"Idade: {root.user_data['age']}"
    Label:
        text: f"Cidade: {root.user_data['city']}"
    Button:
        text: "Atualizar Idade"
        on_press: root.update_age()
